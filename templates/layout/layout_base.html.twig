<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{% block title %}Dev in the hood{% endblock title %}</title>
    <link rel="icon" href="{{ asset('img/favicon.png') }}">

    {% block stylesheets %}
        {% include 'includes/_stylesheets.html.twig' %}
    {% endblock stylesheets %}
</head>
<body class="{% block body_class %}app{% endblock body_class %}">
    {% block body %}
        {% include 'includes/_javascripts.html.twig' %}
    {% endblock body %}

    {% block javascripts %}
        {% include 'includes/_javascripts.html.twig' %}
    {% endblock javascripts %}
</body>
</html>
